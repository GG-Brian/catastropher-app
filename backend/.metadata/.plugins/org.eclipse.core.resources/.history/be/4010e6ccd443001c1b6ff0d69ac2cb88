package com.itc.catastropher.data.model;

import java.io.Serializable;
import java.util.Objects;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class HelpsPrimaryKeys implements Serializable {
	private static final long serialVersionUID = 1L;
	
	@Column(name = "adisaster") int disasterId;
	@Column(name = "agroup") int hgroupId;
	
	
	public HelpsPrimaryKeys() {}
	public HelpsPrimaryKeys(int adisaster, int agroup) {
		super();
		this.adisaster = adisaster;
		this.agroup = agroup;
	}
	
	
	public int getAdisaster() { return adisaster; }
	public void setAdisaster(int adisaster) { this.adisaster = adisaster; }

	public int getAgroup() { return agroup; }
	public void setAgroup(int agroup) { this.agroup = agroup; }
	
	
	@Override public int hashCode() { return Objects.hash(adisaster, agroup); }
	
	@Override public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		HelpsPrimaryKeys other = (HelpsPrimaryKeys) obj;
		return adisaster == other.adisaster && agroup == other.agroup;
	}
}